<resources xmlns:app="http://schemas.android.com/apk/res-auto">
    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.MaterialComponents.Light.NoActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
        <item name="android:textColorHint">@color/colorTextBlack</item>
    </style>

    <style name="MyAutofilledHighlight" parent="AppTheme">
        <item name="android:autofilledHighlight">@drawable/empty_drawable_autofill_highlight</item>
    </style>

    <style name="TextInputLayoutProfile" parent="Widget.Design.TextInputLayout">
        <item name="boxBackgroundColor">@android:color/white</item>
    </style>

</resources>